#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process

# Find the process ID (PID) of the 4-to_infinity_and_beyond process
pid=$(pgrep 4-to_infinity_and_beyond)

# Check if the process exists
if [ -z "$pid" ]; then
    echo "Process 4-to_infinity_and_beyond is not running."
else
    # Terminate the process using the kill command
    kill "$pid"
    echo "Terminated 4-to_infinity_and_beyond process with PID $pid."
fi
